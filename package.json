{
  "name": "mean-start-2",
  "version": "2.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cj-wang/mean-start-2.git"
  },
  "bugs": {
    "url": "https://github.com/cj-wang/mean-start-2/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "start": "concurrently -n client......,......server \"ng serve  --progress false --proxy-config client/proxy.conf.json\" \"npm run server:start:dev\"",
    "build": "rimraf dist && npm run client:build:prod && npm run server:webpack",
    "test": "concurrently -n client......,......server \"ng test --progress false\" \"npm run server:test:watch\"",
    "test:coverage": "rimraf coverage && concurrently -n client......,......server \"ng test --progress false --code-coverage\" \"jest --watch --coverage\"",
    "lint": "npm run client:lint:ci && npm run server:lint",
    "e2e": "npm run client:pree2e && npm run client:e2e && npm run server:test:e2e",
    "conventional-changelog": "conventional-changelog",
    "prepush": "npm run lint",
    "client:start": "ng serve",
    "client:build": "ng build",
    "client:build:prod": "npm run client:build -- --prod --aot",
    "client:test": "ng test",
    "client:test:coverage": "rimraf coverage/client && npm run client:test -- --code-coverage",
    "client:lint": "ng lint",
    "client:lint:fix": "ng lint ngx-admin-demo --fix",
    "client:lint:styles": "stylelint ./client/src/**/*.scss",
    "client:lint:ci": "npm run client:lint && npm run client:lint:styles",
    "client:pree2e": "webdriver-manager update --standalone false --gecko false",
    "client:e2e": "ng e2e",
    "client:docs": "compodoc -p client/src/tsconfig.app.json -d docs",
    "client:docs:serve": "compodoc -p client/src/tsconfig.app.json -d docs -s",
    "client:release:changelog": "npm run conventional-changelog -- -p angular -i client/CHANGELOG.md -s",
    "server:format": "prettier --config server/.prettierrc --write \"server/src/**/*.ts\"",
    "server:start": "ts-node --project server/tsconfig.json -r tsconfig-paths/register server/src/main.ts",
    "server:start:dev": "nodemon --config server/nodemon.json",
    "server:start:debug": "nodemon --config server/nodemon-debug.json",
    "server:prestart:prod": "rimraf dist && tsc --project server",
    "server:start:prod": "node dist/main.js",
    "server:start:hmr": "node dist/server",
    "server:lint": "tslint -p server/tsconfig.json -c server/tslint.json",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:e2e": "jest --config ./server/test/jest-e2e.json",
    "server:webpack": "webpack --config server/webpack.config.js"
  },
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@nebular/auth": "2.0.0-rc.9",
    "@nebular/security": "2.0.0-rc.9",
    "@nebular/theme": "2.0.0-rc.9",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0",
    "bootstrap": "4.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "nebular-icons": "1.0.8",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "reflect-metadata": "^0.1.12",
    "roboto-fontface": "0.8.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "socicon": "3.0.5",
    "typeface-exo": "0.0.22",
    "web-animations-js": "2.2.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@compodoc/compodoc": "1.0.1",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "6.0.90",
    "@types/supertest": "^2.0.5",
    "codelyzer": "4.0.2",
    "concurrently": "^4.0.1",
    "conventional-changelog-cli": "1.3.4",
    "husky": "0.13.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "jest": "^23.5.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "nodemon": "^1.18.3",
    "npm-run-all": "4.0.2",
    "prettier": "^1.14.2",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "stylelint": "7.13.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "3.2.2",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.7.0",
    "tslint-language-service": "0.9.6",
    "typescript": "2.7.2",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../../coverage/server",
    "testEnvironment": "node"
  }
}
