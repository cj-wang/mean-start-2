{
  "name": "mean-start-2",
  "version": "4.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cj-wang/mean-start-2.git"
  },
  "bugs": {
    "url": "https://github.com/cj-wang/mean-start-2/issues"
  },
  "scripts": {
    "ng": "ng",
    "dev": "concurrently -n client......,......server \"ng serve  --progress false\" \"npm run server:start:dev\"",
    "build": "rimraf dist && npm run client:build:prod && npm run client:build:ssr && npm run server:webpack:ssr",
    "start": "node dist/server",
    "test": "concurrently -n client......,......server \"ng test --progress false\" \"npm run server:test:watch\"",
    "test:coverage": "rimraf coverage && concurrently -n client......,......server \"ng test --progress false --code-coverage\" \"jest --watch --coverage\"",
    "lint": "npm run client:lint:ci && npm run server:lint",
    "e2e": "npm run client:pree2e && npm run client:e2e && npm run server:test:e2e",
    "conventional-changelog": "conventional-changelog",
    "prepush": "npm run lint",
    "client:start": "ng serve",
    "client:build": "ng build",
    "client:build:prod": "npm run client:build -- --prod --aot",
    "client:build:ssr": "ng run mean-start-2:server",
    "client:test": "ng test",
    "client:test:coverage": "rimraf coverage/client && npm run client:test -- --code-coverage",
    "client:lint": "ng lint",
    "client:lint:fix": "ng lint mean-start-2 --fix",
    "client:lint:styles": "stylelint ./client/src/**/*.scss",
    "client:lint:ci": "npm run client:lint && npm run client:lint:styles",
    "client:pree2e": "webdriver-manager update --standalone false --gecko false",
    "client:e2e": "ng e2e",
    "client:docs": "compodoc -p client/src/tsconfig.app.json -d docs",
    "client:docs:serve": "compodoc -p client/src/tsconfig.app.json -d docs -s",
    "client:release:changelog": "npm run conventional-changelog -- -p angular -i client/CHANGELOG.md -s",
    "server:build": "rimraf dist/server && tsc -p server/tsconfig.build.json",
    "server:format": "prettier --write \"server/src/**/*.ts\" \"server/e2e/**/*.ts\"",
    "server:start": "ts-node -r tsconfig-paths/register server/src/main.ts",
    "server:start:dev": "tsc-watch -p server/tsconfig.build.json --onSuccess \"node dist/server/src/main.js\"",
    "server:start:debug": "tsc-watch -p server/tsconfig.build.json --onSuccess \"node --inspect-brk dist/server/src/main.js\"",
    "server:start:prod": "node dist/server/src/main.js",
    "server:lint": "tslint -p server/tsconfig.json -c server/tslint.json",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./server/e2e/jest-e2e.json",
    "server:webpack:ssr": "webpack --config server/webpack.ssr.config.js --progress --colors"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@nebular/auth": "4.1.2",
    "@nebular/eva-icons": "4.1.2",
    "@nebular/security": "4.1.2",
    "@nebular/theme": "4.1.2",
    "@nestjs/common": "^6.0.4",
    "@nestjs/core": "^6.0.4",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/microservices": "^6.0.4",
    "@nestjs/ng-universal": "^2.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.4",
    "@nestjs/websockets": "^6.0.4",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "cookie-parser": "^1.4.4",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "localstorage-polyfill": "^1.0.1",
    "nebular-icons": "1.1.0",
    "nest-router": "^1.0.7",
    "node-sass": "^4.12.0",
    "normalize.css": "6.0.0",
    "pace-js": "1.0.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "preboot": "^7.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.3.0",
    "socicon": "3.0.5",
    "typeface-exo": "0.0.22",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular/cli": "^15.2.7",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "8.0.0",
    "@compodoc/compodoc": "1.0.1",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.0.1",
    "conventional-changelog-cli": "1.3.4",
    "husky": "0.13.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "jest": "24.7.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "4.0.2",
    "prettier": "1.17.0",
    "protractor": "5.1.2",
    "stylelint": "7.13.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "^4.4.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../../coverage/server",
    "testEnvironment": "node"
  }
}
